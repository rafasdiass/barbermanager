{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "scripts": {
      "type": "object"
    },
    "private": {
      "type": "boolean"
    },
    "dependencies": {
      "type": "object"
    },
    "devDependencies": {
      "type": "object"
    }
  },
  "required": ["name", "version", "scripts", "private", "dependencies", "devDependencies"]
}
